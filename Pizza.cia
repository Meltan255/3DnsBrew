#include <3ds.h> int main(int argc, char **argv) { // Initialize 3DS services gfxInitDefault(); consoleInit(GFX_TOP, NULL); // Read pizza tower data u8 *pizzaData = (u8*)0x08000000; // Address of pizza tower data in memory // Display pizza tower data on screen printf("Pizza Tower Data:\n"); for (int i = 0; i < 100; i++) { printf("%02X ", pizzaData[i]); if ((i+1) % 16 == 0) printf("\n"); } // Main loop while (aptMainLoop()) { // Check for user input hidScanInput(); u32 kDown = hidKeysDown(); if (kDown & KEY_START) break; // Exit program if START is pressed // Flush and swap framebuffers gfxFlushBuffers(); gfxSwapBuffers(); gspWaitForVBlank(); } // Exit program gfxExit(); return 0; }
